    "body-parser": "^1.18.3",
    "clsx": "^1.0.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "downshift": "^3.2.7",
    "express": "^4.16.4",
    "express-history-api-fallback": "^2.2.1",
    "isomorphic-fetch": "2.2.1",
    "lodash": "^4.17.11",
    "markdown-to-jsx": "^6.9.3",
    "mobx": "^5.9.4",
    "mobx-react-lite": "^1.2",
    "npm-run-all": "^4.1.5",
    "query-string": "^6.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
