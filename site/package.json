{
  "private": true,
  "name": "wf-stuffs",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel dev:**",
    "dev:app": "cross-env NODE_ENV=development nodemon index.js",
    "dev:lambda": "netlify-lambda serve serverless -c ./webpack/serverless.js",
    "build": "npm-run-all --parallel build:**",
    "build:app": "cross-env NODE_ENV=production node ./index.js",
    "build:functions": "netlify-lambda build serverless -c ./webpack/serverless.js",
    "prtya": "prettier --write src/**/*.js src/**/*.css src/**/*.ts src/**/*.tsx --write webpack/**/*.js",
    "prtyq": "pretty-quick --pattern '**/*.*(js|jsx)' --pattern '**/*.*(ts|tsx)'  --pattern '**/*.*json'",
    "lint": "tslint src/*.ts{,x} src/**/*.ts{,x}",
    "lintfix": "tslint --fix -c ./tslint.json 'src/**/*{.ts,.tsx}'",
    "check": "tsc",
    "bundleanal": "webpack-bundle-size-analyzer webpack.stats.json",
    "external": "node -r esm webpack/deps.js",
    "parcel": "parcel build src/index.html --out-dir public",
    "deploy": "netlify deploy --dir=dist/public --prod",
    "draft": "netlify deploy --dir=dist/public "
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.3",
    "@types/node": "^11.13.4",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "barrelbot": "^0.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "commonmark": "^0.29.0",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "esm": "^3.2.22",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^3.0.1",
    "fuse-box": "^3.7.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "netlify-lambda": "^1.4.3",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.11",
    "parcel-bundler": "^1.12.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "pushstate-server": "^3.1.0",
    "raw-loader": "^2.0.0",
    "react-markdown-loader": "^1.2.0",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "terser-webpack-plugin": "^1.2.3",
    "tslint": "^5.15.0",
    "typescript": "^3.4.3",
    "webpack": "4.30.0",
    "webpack-bundle-size-analyzer": "^3.0.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "3.3.1",
    "webpack-modules": "^1.0.0",
    "winston": "^3.2.1"
  },
  "dependencies": {
    "@loadable/component": "^5.7.0",
